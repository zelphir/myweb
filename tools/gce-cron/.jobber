[prefs]
notifyProgram: /usr/local/bin/jobberNotifier.sh

[jobs]
- name: ImportStats
  cmd: "cd ~ && node import.js"
  time: "0 2 0-1 * * *"
  onError: Continue
  notifyOnError: true
  notifyOnSuccess: true
  notifyOnFailure: true
- name: ImportStats
  cmd: "cd ~ && node import.js"
  time: "*/10 * 9-18 * * *"
  onError: Continue
  notifyOnError: true
  notifyOnSuccess: true
  notifyOnFailure: true
- name: ImportStats
  cmd: "cd ~ && node import.js"
  time: "0 1 18-23 * * *"
  onError: Continue
  notifyOnError: true
  notifyOnSuccess: true
  notifyOnFailure: true
- name: CleanLanguages
  cmd: "cd ~ && node cleanup.js"
  time: "0 30 2 * * *"
  onError: Continue
  notifyOnError: true
  notifyOnSuccess: true
  notifyOnFailure: true
